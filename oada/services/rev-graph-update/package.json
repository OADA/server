    "randomstring": "^1.3.0",
    "tslib": "2.6.3"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@oada/client": "^5.1.0",
    "@oada/write-handler": "workspace:^",
    "@types/debug": "^4.1.12",
