    "@oada/well-known-json": "^4.0.2",
    "@qlever-llc/interface2class": "^1.0.0",
    "@types/ejs": "^3.1.2",
    "arangojs": "^8.3.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "clone-deep": "^4.0.1",
    "debug": "^4.3.4",
