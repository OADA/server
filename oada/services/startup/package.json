  "dependencies": {
    "@oada/lib-arangodb": "workspace:^",
    "@oada/lib-kafka": "workspace:^",
    "@oada/pino-debug": "workspace:^",
    "debug": "^4.3.7",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
