{
  "name": "@oada/lib-prom",
  "version": "3.6.1",
  "description": "OADA Prometheus library",
  "main": "./dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "yarn g:tsc",
    "clean": "yarn run build --clean",
    "test": "yarn run build test && ava"
  },
  "ava": {
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OADA/@oada/lib-kafka.git"
  },
  "author": "Alex Layton <alex@layton.in>",
  "license": "Apache-2.0",
  "dependencies": {
    "@oada/lib-config": "^3.6.1",
    "prom-client": "^14.1.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/convict": "^6.1.1",
    "@types/node": "^18.11.18",
    "@types/ws": "^8.5.4",
    "ava": "5.1.1",
    "fastify-plugin": "^4.5.0"
  },
  "volta": {
    "node": "18.13.0"
  },
  "optionalDependencies": {
    "nstats": "^5.0.0"
  }
}
