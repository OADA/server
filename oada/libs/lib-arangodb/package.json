{
  "name": "@oada/lib-arangodb",
  "version": "3.0.1",
  "description": "All OADA Arango interface code in one place.",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "contributors": [
    "Alex Layton <alex@layton.in> (https://github.com/awlayton)",
    "Andrew Balmos <andrew@balmos.org> (https://github.com/abalmos)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OADA/@oada/lib-arangodb.git"
  },
  "author": "Aaron Ault <aultac@gmail.com> (https://github.com/aultac)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OADA/@oada/lib-arangodb/issues"
  },
  "homepage": "https://github.com/OADA/@oada/lib-arangodb#readme",
  "keywords": [
    "oada",
    "arangodb",
    "lib"
  ],
  "scripts": {
    "build": "yarn g:tsc",
    "init": "node dist/init.js",
    "test": "yarn run build test && ava"
  },
  "ava": {
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "dependencies": {
    "@oada/lib-config": "^3.0.1",
    "@oada/oadaify": "^2.1.0",
    "@oada/types": "^2.0.1",
    "arangojs": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "clone-deep": "^4.0.1",
    "debug": "^4.3.4",
    "deep-equal": "^2.0.5",
    "flat": "^5.0.2",
    "json-ptr": "^3.1.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/clone-deep": "^4.0.1",
    "@types/convict": "^6.1.1",
    "@types/debug": "^4.1.7",
    "@types/deep-equal": "^1.0.1",
    "@types/flat": "^5.0.2",
    "@types/json-pointer": "^1.0.31",
    "@types/node": "^16.11.27",
    "ava": "4.0.0-rc.1",
    "type-fest": "^2.12.2"
  }
